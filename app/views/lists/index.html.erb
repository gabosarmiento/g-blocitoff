<title>Lists </title>
<h1>My Lists</h1>
<div>
<%= form_for @list do |f| %>
  <%= f.text_field :name %>
  <%= f.submit "Add to-do list", class: "btn btn-primary " %>
    <% end %>
</div>
<div class="well">
<h4>All my lists here</h4>
<% @lists.each do |list| %>
  <li> <%= link_to list.name, list %> </li>
<% end %>
</div>